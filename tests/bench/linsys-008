% x1 = 11 / x2 = 1 / x3 = 3 / x4 = 5 / x5 = 1 / x6 = 12 / x7 = 0 / x8 = 10 / 
assert
  7*x1 -10*x2 +6*x3 -10*x4 -6*x5 +2*x6 +7*x7 +5*x8 = 103.
assert
  2*x1 -1*x2 -10*x3 -10*x4 +5*x5 +2*x6 +5*x7 -6*x8 = -90.
assert
  0*x1 +9*x2 +0*x3 -9*x4 +7*x5 +1*x6 -2*x7 -9*x8 = -107.
assert
 -5*x1 +3*x2 -4*x3 -8*x4 +4*x5 -4*x6 +9*x7 +3*x8 = -118.
assert
 -6*x1 +6*x2 -7*x3 +7*x4 +0*x5 -5*x6 +9*x7 -6*x8 = -166.
assert
 -6*x1 +7*x2 -1*x3 -8*x4 -8*x5 +5*x6 -7*x7 -9*x8 = -140.
assert
 -2*x1 -5*x2 -4*x3 +0*x4 +1*x5 -10*x6 +1*x7 -8*x8 = -238.
assert
  3*x1 -8*x2 -8*x3 +4*x4 +8*x5 +2*x6 -2*x7 +2*x8 = 73.
