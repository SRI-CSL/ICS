can
(((p(0) <=> p(1)) => (p(0) & p(1) & p(2) & p(3) & p(4))) &
 ((p(1) <=> p(2)) => (p(0) & p(1) & p(2) & p(3) & p(4))) &
 ((p(2) <=> p(3)) => (p(0) & p(1) & p(2) & p(3) & p(4))) &
 ((p(3) <=> p(4)) => (p(0) & p(1) & p(2) & p(3) & p(4))) &
 ((p(4) <=> p(0)) => (p(0) & p(1) & p(2) & p(3) & p(4))))
     =>(p(0) & p(1) & p(2) & p(3) & p(4)).
