% Examples of formulae true in intuitionistic propositional logic
% from Roy Dyckhoff, University of St Andrews
% http://www-theory.dcs.st-and.ac.uk/~rd/logic/marks.html

% de Bruijn formulae

can ((b<=>c)=>(a&b&c)) & ((c<=>a)=>(a&b&c)) & ((a<=>b)=>(a&b&c)) => (a&b&c).

can true & ((p(1) <=> p(2)) => p(1) & p(2) & p(3) & p(4) & p(5)) & ((p(2) <=> p(3)) => p(1) & p(2) & p(3) & p(4) & p(5)) & ((p(3) <=> p(4)) => p(1) & p(2) & p(3) & p(4) & p(5)) & ((p(4) <=> p(5)) => p(1) & p(2) & p(3) & p(4) & p(5)) & ((p(5) <=> p(1)) => p(1) & p(2) & p(3) & p(4) & p(5)) => p(1) & p(2) & p(3) & p(4) & p(5).

can true & ((p(1) <=> p(2)) => p(1) & p(2) & p(3) & p(4)) & ((p(2) <=> p(3)) => p(1) & p(2) & p(3) & p(4)) & ((p(3) <=> p(4)) => p(1) & p(2) & p(3) & p(4)) & ((p(4) <=> p(1)) => p(1) & p(2) & p(3) & p(4)) => p(0) | p(1) & p(2) & p(3) & p(4) | ~p(0).

% Pigeonhole formulae

can
  (o(1, 1) | o(1, 2) | o(1, 3)) & (o(2, 1) | o(2, 2) | o(2, 3)) & (o(3, 1) | o(3, 2) | o(3, 3)) & (o(4, 1) | o(4, 2) | o(4, 3)) => o(1, 1) & o(2, 1) | o(1, 1) & o(3, 1) | o(1, 1) & o(4, 1) | o(2, 1) & o(3, 1) | o(2, 1) & o(4, 1) | o(3, 1) & o(4, 1) | o(1, 2) & o(2, 2) | o(1, 2) & o(3, 2) | o(1, 2) & o(4, 2) | o(2, 2) & o(3, 2) | o(2, 2) & o(4, 2) | o(3, 2) & o(4, 2) | o(1, 3) & o(2, 3) | o(1, 3) & o(3, 3) | o(1, 3) & o(4, 3) | o(2, 3) & o(3, 3) | o(2, 3) & o(4, 3) | o(3, 3) & o(4, 3).
      
can
(o(1, 1) | ~~o(1, 2)) & (o(2, 1) | ~~o(2, 2)) & (o(3, 1) |
 ~~o(3, 2)) => o(1, 1) & o(2, 1) | o(1, 1) & o(3, 1) | o(2, 1) & o(3, 1) |
 o(1, 2) & o(2, 2) | o(1, 2) & o(3, 2) | o(2, 2) & o(3, 2).
 
% formulae requiring contractions

can ~ ~(~p1 | p1).

can ~ ~(~p1 | ~p2 | ~p3 | (p1 & p2 & p3)).

can ((((p=>q)=>p)=>p)=>q)=>q.

can (((p1 & p2) | (p1=>f) | (p2=>f))=>f)=>f.

can (((p1 & p2) | (~ ~p1=>f) | (p2=>f)) =>f) =>f.

% Equivalences

can (a(1)<=>a(2))<=>(a(2)<=>a(1)).

can ((a(1)<=>a(2))<=>a(3))<=>(a(3)<=>(a(2)<=>a(1))).

% Formulae of Schwichtenberg with big NJ deductions

can
p(3)&(p(1)=>p(1)=>p(0))&(p(2)=>p(2)=>p(1))&(p(3)=>p(3)=>p(2))=>p(0).

can
~ ~p(3) & (p(1)=>p(1)=>p(0))&(p(2)=>p(2)=>p(1))&(p(3)=>p(3)=>p(2))=>p(0).

%   Formulae of Korn & Kreitz

can
((a(0) => f) & ((b(3) => b(0)) => a(3)) & ((b(0) => a(1)) => a(0)) & ((b(1) => a(2)) => a(1)) & ((b(2) => a(3)) => a(2)) => f)  &  (((b(2) => a(3)) => a(2)) & (((b(1) => a(2)) => a(1)) & (((b(0) => a(1)) => a(0)) & (((b(3) => b(0)) => a(3)) & (a(0) => f)))) => f).

can
(a(0) => f) & ((~~b(3) => b(0)) => a(3)) & ((~~b(0) => a(1)) => a(0)) & ((~~b(1) => a(2)) => a(1)) & ((~~b(2) => a(3)) => a(2)) => f.

can (((p=>q)=>p)=>p).
